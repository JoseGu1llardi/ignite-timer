{
  "extends": "@rocketseat/eslint-config/react",
  "import/no-extraneous-dependencies": [
    "error",
    { "packageDir": "./", "extensions": [".js", ".jsx", ".ts", ".tsx"] }
  ],
  "import/resolver": {
    "node": {
      "extensions": [".js", ".jsx", ".ts", ".tsx"],
      "moduleDirectory": ["node_modules", "src/"]
    }
  }
}
